<RaprDictionary>
    <namespace>
    <label>SystemDefaults</label>
        <!-- Application wide defaults -->
        <item>
            <!-- Default retry interval for interrogative events -->
            <field>RETRYINTERVAL</field>
            <value>10</value>
        </item>
        <item>
            <!-- Default number of retries for interrogative events -->
            <field>NUMRETRIES</field>
            <value>3</value>
        </item>
        <item>
            <!-- Default pattern for all events -->
            <field>PATTERN</field>
            <value>PERIODIC [1 1024]</value>
        </item>
        <item>
            <!-- Default stream duration for interrogative and declarative events  -->
            <field>STREAMDURATION</field>
            <value>.99</value>
        </item>
        <item>
            <!-- Default protocol for all events -->
            <field>PROTOCOL</field>
            <value>UDP</value>
        </item>
    </namespace>
</RaprDictionary>
